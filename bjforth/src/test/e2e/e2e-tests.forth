###################################################################################################
# Defining words
###################################################################################################

: MULTIPLY * ;
: TIMES-10 10 MULTIPLY ;
20 TIMES-10 PRINTLN
50 50 + PRINTLN

###################################################################################################
# Redefining words
###################################################################################################

: SAY-HELLO ." Hello ". PRINT ;
: SAY-HELLO SAY-HELLO ." , world ". PRINTLN ; 
SAY-HELLO

###################################################################################################
# Strings
###################################################################################################

." Hello, world ". PRINTLN
: PRINT-ME ." Hello, world ". PRINTLN ;
PRINT-ME

###################################################################################################
# Java inter-op - Static methods
###################################################################################################

# Integer.valueOf("12")
: INTEGER-VALUE-OF
  ,< Integer/valueOf(String)/1 >, 
  PRINTLN
;
." 12 ". INTEGER-VALUE-OF 

# String.join(", ", "Hello", "world")
: STRING-JOIN 
  ,< String/join(CharSequence, CharSequence...)/3 >, 
  PRINTLN
;
." world ". ." Hello ". ." ,  ". STRING-JOIN  

###################################################################################################
# Java inter-op - Instance methods
###################################################################################################

: LONG-VALUE
  .< longValue()/0 >.
  PRINTLN
;

24 LONG-VALUE

###################################################################################################
# Java inter-op - Constructor
###################################################################################################

: NEW-STRING
  @< String(String)/1 >@
  PRINTLN
;

." Hello, world ". NEW-STRING 

